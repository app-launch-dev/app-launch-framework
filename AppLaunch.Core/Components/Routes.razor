<Router AppAssembly="typeof(Program).Assembly" AdditionalAssemblies="new[] { typeof(AppLaunch.Admin._Imports).Assembly}">
    <Found Context="routeData">
        <AuthorizeRouteView RouteData="routeData" DefaultLayout="typeof(AppLaunch.Themes.Layout.AppLaunchDefault)">
            <NotAuthorized>
                <RedirectToLogin />
            </NotAuthorized>
        </AuthorizeRouteView>
        @* <FocusOnNavigate RouteData="routeData" Selector="h1" /> *@
    </Found>
</Router>

